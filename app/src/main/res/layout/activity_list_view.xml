<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sarita.medinet.ListView"
    android:background="#d5d4fb"

    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="2dp"
        android:background="#C0C0C0"
        android:text=""
        android:id="@+id/textView399"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Doctor Detail"
        android:onClick="Doctor"
        android:padding="15dp"
        android:id="@+id/tvDoctorDetail"
        android:gravity="center"
        android:layout_below="@+id/textView399"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="2dp"
        android:background="#C0C0C0"
        android:text=""
        android:id="@+id/textView5"
        android:layout_below="@+id/tvDoctorDetail"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Appointment"
        android:onClick="Appointment"
        android:padding="15dp"
        android:gravity="center"
        android:id="@+id/tvAppointment"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#C0C0C0"
        android:textSize="2dp"
        android:text=""
        android:id="@+id/textView4"
        android:layout_below="@+id/tvAppointment"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:padding="15dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="About Medinet"
        android:id="@+id/tvAbout"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="2dp"
        android:background="#C0C0C0"
        android:text=""
        android:id="@+id/textView8"
        android:layout_below="@+id/tvAbout"/>




</RelativeLayout>
